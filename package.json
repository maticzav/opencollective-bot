{
  "name": "opencollective-bot",
  "version": "0.0.0-semantic-release",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "ts-node src/index.ts",
    "test": "NODE_ENV=test jest",
    "coverage": "yarn codecov",
    "deploy:staging": "now -e APP_ID=@oc-bot-staging-app-id -e WEBHOOK_SECRET=@oc-bot-staging-webhook-secret -e PRIVATE_KEY=@oc-bot-staging-private-key -e SENTRY_DSN=@oc-bot-staging-sentry-dsn --target staging && now alias bot-staging.opencollective.com",
    "deploy:production": "now --target production"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "joi": "^14.3.1",
    "lodash.uniq": "^4.5.0",
    "multilines": "^1.0.2",
    "node-fetch": "^2.3.0",
    "probot": "^9.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/btoa": "1.2.3",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.12",
    "@types/joi": "14.3.3",
    "@types/js-yaml": "3.12.1",
    "@types/lodash.uniq": "4.5.6",
    "@types/nock": "10.0.2",
    "@types/node": "11.13.10",
    "@types/node-fetch": "2.3.3",
    "@types/request-promise-native": "1.0.16",
    "@zeit/ncc": "0.18.5",
    "btoa": "1.2.1",
    "codecov": "3.4.0",
    "husky": "2.2.0",
    "jest": "24.8.0",
    "js-yaml": "3.13.1",
    "nock": "10.0.6",
    "npm-run-all": "4.1.5",
    "prettier": "1.17.0",
    "pretty-quick": "1.10.0",
    "request-promise-native": "1.0.7",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "typescript": "3.4.5"
  },
  "license": "MIT"
}
